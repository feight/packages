  "files": [
    "lib"
  ],
  "dependencies": {
    "@types/text-table": "0.2.2",
    "chalk": "4.1.1",
    "commander": "8.0.0",
    "core-js": "3.15.2",
    "folder-hash": "4.0.1",
    "fs-extra": "10.0.0",
    "globby": "11.0.4",
    "install-local": "3.0.1",
