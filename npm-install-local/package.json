  "devDependencies": {
    "@newsteam/babel": "0.0.39",
    "@newsteam/eslint": "0.0.52",
    "@newsteam/jasmine": "0.0.36",
    "@types/fs-extra": "9.0.12",
    "@types/node": "16.3.2",
