{
  "name": "@newsteam/npm-install-local",
  "version": "0.0.3",
  "description": "NPM linking tool for local development",
  "main": "lib",
  "types": "lib",
  "sideEffects": false,
  "scripts": {
    "build": "bash ../build.sh",
    "prepublishOnly": "bash ../publish.sh",
    "test": "bash ../test.sh",
    "update": "rm -rf node_modules && rm -f package-lock.json && ncu -u && npm install"
  },
  "bin": {
    "npm-install-local": "./lib/cli/index.js"
  },
  "browserslist": "node 12",
  "engines": {
    "node": ">= 12"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/feight/packages.git"
  },
  "author": "News Team (https://www.newsteam.co.za)",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/feight/packages/issues"
  },
  "homepage": "https://github.com/feight/packages/tree/master/npm-install-local#readme",
  "files": [
    "lib"
  ],
  "dependencies": {
    "@types/text-table": "0.2.1",
    "chalk": "4.1.0",
    "commander": "6.2.1",
    "core-js": "3.8.2",
    "folder-hash": "4.0.0",
    "fs-extra": "9.0.1",
    "globby": "11.0.2",
    "install-local": "3.0.1",
    "node-pty": "0.9.0",
    "object-hash": "2.1.1",
    "rc-config-loader": "4.0.0",
    "regenerator-runtime": "0.13.7",
    "text-table": "0.2.0",
    "through2": "4.0.2"
  },
  "devDependencies": {
    "@newsteam/babel": "0.0.23",
    "@newsteam/eslint": "0.0.25",
    "@newsteam/jasmine": "0.0.22",
    "@types/fs-extra": "9.0.6",
    "@types/node": "14.14.20",
    "@types/object-hash": "1.3.4",
    "@types/through2": "2.0.36",
    "npm-check-updates": "^10.2.5"
  }
}
