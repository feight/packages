    "stylelint-no-unsupported-browser-features": "4.1.4",
    "stylelint-order": "4.1.0",
    "stylelint-scss": "3.19.0",
    "stylelint-selector-tag-no-without-class": "2.0.3",
    "stylelint-use-nesting": "3.0.0"
  }
