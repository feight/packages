    "build": "bash ../build.sh",
    "prepublishOnly": "bash ../publish.sh && stylelint **/*.scss && stylelint-find-new-rules",
    "test": "bash ../test.sh && stylelint **/*.scss && stylelint-find-new-rules",
    "update": "ncu -u"
  },
  "browserslist": "node 12",
  "engines": {
    "node": ">= 12"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/feight/packages"
  },
  "author": "News Team (https://www.newsteam.io)",
  "contributors": [
    {
      "name": "Peter Bodechtel",
      "url": "https://www.sweetlikepete.com"
    }
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/feight/packages/issues"
  },
  "keywords": [
    "stylelint",
    "stylelintconfig",
    "stylelint-config",
    "newsteam",
    "@newsteam"
  ],
  "homepage": "https://github.com/feight/packages/tree/master/stylelint-config#readme",
  "files": [
    "lib"
  ],
  "dependencies": {
    "@newsteam/jasmine": "0.0.24"
  },
  "devDependencies": {
    "@newsteam/babel": "0.0.25",
