    "url": "git+https://github.com/feight/packages.git"
  },
  "author": "News Team (https://www.newsteam.co.za)",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/feight/packages/issues"
  },
  "homepage": "https://github.com/feight/packages/tree/master/package-json#readme",
  "files": [
    "lib"
  ],
  "dependencies": {
    "fs-extra": "9.0.1"
  },
  "devDependencies": {
    "@newsteam/babel": "0.0.23",
    "@newsteam/eslint": "0.0.25",
    "@newsteam/jasmine": "0.0.22",
    "@newsteam/npm-install-local": "0.0.3",
    "@types/fs-extra": "9.0.6",
    "@types/node": "14.14.20",
    "npm-check-updates": "^10.2.5",
    "typescript": "4.1.3"
  }
