{
  "name": "@newsteam/package-json",
  "version": "0.0.1",
  "description": "Assemble Schema",
  "main": "lib",
  "types": "lib",
  "sideEffects": false,
  "scripts": {
    "build": "bash ../build.sh",
    "clean": "rm -rf lib",
    "npm-install-local": "npm-install-local",
