{
  "name": "@newsteam/legacy-cli",
  "version": "0.0.1",
  "description": "Newsteam command line tools",
  "main": "lib",
  "types": "lib",
  "sideEffects": false,
  "scripts": {
    "build": "bash ../build.sh",
    "npm-install-local": "npm-install-local",
    "prepublishOnly": "bash ../publish.sh",
    "test": "bash ../test.sh",
    "tsc": "tsc --emitDeclarationOnly",
    "update": "ncu -u"
  },
  "bin": {
    "newsteam": "./lib/cli/index.js"
  },
  "browserslist": "node 12",
  "engines": {
    "node": ">= 12"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/feight/packages.git"
  },
  "author": "News Team (https://www.newsteam.io)",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/feight/packages/issues"
  },
  "homepage": "https://github.com/feight/packages/tree/master/legacy-cli#readme",
  "files": [
    "lib"
  ],
  "dependencies": {
    "asciichart": "1.5.25",
    "commander": "6.2.1",
    "core-js": "3.8.2",
    "fs-extra": "9.0.1",
    "glob-promise": "3.4.0",
    "globby": "11.0.2",
    "gulp": "4.0.2",
    "is-equal": "1.6.1",
    "js-yaml": "4.0.0",
    "jsonschema": "1.4.0",
    "regenerator-runtime": "0.13.7",
    "speed-measure-webpack-plugin": "1.3.3",
    "to-snake-case": "1.0.0",
    "ts-node": "9.1.1",
    "webpack": "5.14.0"
  },
  "devDependencies": {
    "@newsteam/babel": "0.0.23",
    "@newsteam/jasmine": "0.0.22",
    "@newsteam/npm-install-local": "0.0.3",
    "@types/asciichart": "1.5.4",
    "@types/fs-extra": "9.0.6",
    "@types/gulp": "4.0.8",
    "@types/js-yaml": "4.0.0",
    "@types/jsonschema": "1.1.1",
    "@types/node": "14.14.20",
    "@types/to-snake-case": "1.0.0",
    "npm-check-updates": "^10.2.5"
  },
  "localDependencies": {
