    "@newsteam/legacy-webpack-config": "../legacy-webpack-config",
    "@newsteam/legacy-service-worker": "../legacy-service-worker",
    "@newsteam/legacy-settings": "../legacy-settings",
    "@newsteam/legacy-cli-errors": "../legacy-cli-errors",
    "@newsteam/legacy-cli-logger": "../legacy-cli-logger",
    "@newsteam/cli-notify": "../cli-notify",
    "@newsteam/cli-tasks": "../cli-tasks",
    "@newsteam/cli-utils": "../cli-utils",
    "@newsteam/schema": "../schema"
  }
}
