    "is-equal": "1.6.3",
    "js-yaml": "4.1.0",
    "jsonschema": "1.4.0",
    "regenerator-runtime": "0.13.9",
    "speed-measure-webpack-plugin": "1.5.0",
    "to-snake-case": "1.0.0",
