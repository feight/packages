  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/feight/packages/issues"
  },
  "homepage": "https://github.com/feight/packages/tree/master/legacy-cli#readme",
  "files": [
    "lib"
  ],
  "dependencies": {
    "@newsteam/cli-notify": "0.0.11",
    "@newsteam/cli-tasks": "^0.0.43",
    "@newsteam/cli-utils": "0.0.15",
    "@newsteam/legacy-cli-config": "0.0.23",
    "@newsteam/legacy-cli-errors": "0.0.12",
    "@newsteam/legacy-cli-logger": "0.0.14",
